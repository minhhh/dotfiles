{
  "security": {
    "auth": {
      "selectedType": "oauth-personal"
    }
  },
  "hasSeenIdeIntegrationNudge": true,
  "ide": {
    "enabled": true
  },
  "mcpServers": {
    "local-excel-new": {
      "command": "uvx",
      "args": [
        "excel-mcp-server",
        "stdio"
      ],
      "env": {
        "EXCEL_FILES_PATH": "/Users/minh"
      }
    }
  },
  "ui": {
    "theme": "Default Light"
  },
  "general": {
    "sessionRetention": {
      "enabled": true,
      "maxAge": "30d",
      "warningAcknowledged": true
    }
  }
}
