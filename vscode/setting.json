{
  "editor.fontFamily": "'Monolisa-Medium', 'Source Code Pro Medium', 'Hack'",
  "editor.fontSize": 12,
  "editor.renderWhitespace": "boundary",
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/**": true,
    "**/tmp/**": true,
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/node_modules": true,
    "**/bower_components": true,
    "**/dist/**": true,
    "**/log/**": true,
    "**/logs/**": true,
    "**/.fdk/**": true
  },

  "workbench.colorTheme": "Quiet Light",
  "editor.minimap.enabled": false,

  "workbench.editor.enablePreview": true,
  "workbench.editor.enablePreviewFromQuickOpen": false,

  "vim.easymotion": true,
  "vim.sneak": true,
  "vim.incsearch": true,
  "vim.useSystemClipboard": true,
  "vim.useCtrlKeys": true,
  "vim.hlsearch": true,
  "vim.insertModeKeyBindings": [
    {
      "before": ["j", "k"],
      "after": ["<Esc>"]
    },
    {
      "before": ["k", "j"],
      "after": ["<Esc>"]
    },
    {
      "before": ["<C-v>"],
      "after": [],
      "commands": "editor.action.clipboardPasteAction"
    }
  ],
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["<C-n>"],
      "commands": [
        {
          "command": ":nohl"
        }
      ]
    },
    {
      "before": ["s"],
      "after": ["x", "i"]
    },
    {
      "before": ["S"],
      "after": ["0", "D", "i"]
    }
  ],
  "vim.visualModeKeyBindings": [
    {
      "before": ["s"],
      "after": ["x", "i"]
    }
  ],
  "vim.leader": "-",
  "vim.handleKeys": {
    "<C-a>": false,
    "<C-f>": false
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.tabSize": 2,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.tabSize": 2,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "[haxe]": {
    "editor.tabCompletion": "on",
    "editor.suggestSelection": "first"
  },
  "eslint.workingDirectories": [
    {
      "mode": "auto"
    }
  ],
  "java.project.importHint": false,
  "python.languageServer": "Default",
  "python.condaPath": "~/miniconda3/bin/conda",
  "emmet.showSuggestionsAsSnippets": true,
  "files.associations": {
    "*html": "html",
    "*njk": "html",
    "*.s": "racket"
  },
  "C_Cpp.updateChannel": "Insiders",
  "editor.cursorStyle": "line",
  "editor.lineNumbers": "on",
  "editor.wordSeparators": "/\\()\"':,.;<>~!@#$%^&*|+=[]{}`?-",
  "editor.wordWrap": "off",
  "files.exclude": {
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true
  },
  "[solidity]": {
    "editor.defaultFormatter": "JuanBlanco.solidity"
  },
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[scss]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[yaml]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.inlineSuggest.enabled": true,
  "dashboard.projectData": null,
  "go.formatTool": "gofmt",
  "explorer.confirmDelete": false,
  "terminal.integrated.inheritEnv": false,
  "github.copilot.enable": {
    "*": true,
    "yaml": true,
    "plaintext": false,
    "markdown": true
  },
  "[python]": {
    "editor.formatOnType": true
  },
  "solidity.compileUsingRemoteVersion": "v0.8.17+commit.8df45f5f",
  "[cpp]": {
    "editor.wordBasedSuggestions": false,
    "editor.suggest.insertMode": "replace",
    "editor.semanticHighlighting.enabled": true
  },
  "C_Cpp.clang_format_fallbackStyle": "{ BasedOnStyle: Google, IndentWidth: 2, ColumnLimit: 0}",
  "cmakeFormat.exePath": "/Users/minhha/.nix-profile/bin/cmake-format"
}
